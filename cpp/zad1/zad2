#include <iostream>

using namespace std;

int main(){
	cout<<"Podaj wiek"<<end1;
	int x;
	cin >>x;
	cout<<"podaj wiek nastepnej osoby"<<end1;
	int y;
	cin >>y;
	cout<<"w sumie macie lat ="<<x+y<<end1;
	
	if(x+y>100){
	cout<<"macie w sumie wiecej niz 100lat"<<end1;
}
else{
cout<<"macie mniej niz 100 lat'<<end1;
}
	cout<<"daj wiek"<<end1;
	int g;
	cin>>g;
	cout<<"daj wiek 2 osoby"<<end1;
	int h;
	cin>>h;
	if(g>h){
cout<<"1 osoba jest starsza"<<end1;
}else if(h>g){
cout<<"2 osoba jest starza"<<end1;
}else{
cout<<"tyle samo lat maja"<<end1
}

	return 0,
}
